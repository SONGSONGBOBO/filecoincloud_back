<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.songbo.filecoincloud.mapper.FccUserMapper">

    <select id="getByName" resultType="com.songbo.filecoincloud.entity.FccUser">
        select * from fcc_user where fcc_user_name = #{name}
    </select>
</mapper>
